@model frame.Models.Category
 
@{
    ViewBag.Title = "CreateCategory";
    Layout = "~/Views/SharedAdmin/_Layout.cshtml";
}
             <div class="back">
                <a href="../Admin/CategoryManagement"><h6>Back to list</h6></a>
            </div> 
@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        <div class="khungCate">

        <h4>Create Category</h4>
        <hr />
        @Html.ValidationSummary(true)
            <div class="CreateContent">
        <div class="form-group">
            <label class= "col-md-10">Category ID</label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.idCategory)
                @Html.ValidationMessageFor(model => model.idCategory)
            </div>
        </div>
 
        <div class="form-group">
            <label class= "col-md-10">Category Name</label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.nameCategory)
                @Html.ValidationMessageFor(model => model.nameCategory)
            </div>
        </div>
 
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
        </div>
        </div>
    </div>
}
 



